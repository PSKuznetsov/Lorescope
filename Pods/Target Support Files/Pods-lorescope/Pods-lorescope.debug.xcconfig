GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/Realm" "${PODS_ROOT}/Headers/Public/SVProgressHUD"
LIBRARY_SEARCH_PATHS = $(inherited) "$CONFIGURATION_BUILD_DIR/Realm" "$CONFIGURATION_BUILD_DIR/SVProgressHUD" $(inherited) "${PODS_ROOT}/Realm/core"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/Realm" -isystem "${PODS_ROOT}/Headers/Public/SVProgressHUD"
OTHER_LDFLAGS = $(inherited) -ObjC -l"Realm" -l"SVProgressHUD" -l"c++" -l"realm-ios" -framework "QuartzCore"
PODS_ROOT = ${SRCROOT}/Pods
